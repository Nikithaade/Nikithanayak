	#post-5598 > div > div > div.wpb_column.vc_column_container.vc_col-sm-4 > div > div > div.wpb_single_image.wpb_content_element.vc_align_left.offset-image-perfectxl > figure > div > img {
		max-width: 30% !important;
	}
	
	#post-5598 > div > div > div.wpb_column.vc_column_container.vc_col-sm-4 > div > div > div:nth-child(2) {
		max-height: 20px !important;
	}
	
	#post-5598 > div > div > div.wpb_column.vc_column_container.vc_col-sm-4 {
